        stage('Test') {
            steps {
                withCredentials([string(credentialsId: 'credTelegram', variable: 'USERNAME')]) {
                    sh """
                    'python ./hello_world.py --data $TELEGRAM'
                    """
                }
            }
        }
    }
}
