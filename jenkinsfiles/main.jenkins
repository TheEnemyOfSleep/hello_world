pipeline {
    agent { docker { image 'python:3.8.1' } }
    parameters {
        password(name: 'TELEGRAM', defaultValue: '', description: 'Slack Token')
    }
    stages {
        stage('Test') {
            steps {
                sh """
                python ./hello_world.py --data '${params.TELEGRAM}'
                """
            }
        }
    }
}
